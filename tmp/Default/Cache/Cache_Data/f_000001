	COLOR: #000000; TEXT-DECORATION: none
/* 	COLOR: #ffffff; TEXT-DECORATION: none - SCT V7 default */
}

.tabon A:visited {
	COLOR: #000000; TEXT-DECORATION: none
/*	COLOR: #ffffff; TEXT-DECORATION: none - SCT V7 default */
}

.tabon A:active {
	COLOR: #000000; TEXT-DECORATION: none
/*	COLOR: #ffffff; TEXT-DECORATION: none - SCT V7 default */
}

.tabon A:hover {
	COLOR: #000000; TEXT-DECORATION: none
/*	COLOR: #ffffff; TEXT-DECORATION: none - SCT V7 default */
}

.taboff {
	PADDING-LEFT: 8px; 
